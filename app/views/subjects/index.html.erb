<p id="notice"><%= notice %></p>

<h1>Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Term</th>
      <th>Name</th>
      <th>Abbreviation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.term %></td>
        <td><%= subject.name %></td>
        <td><%= subject.abbreviation %></td>
        <td><%= link_to 'Show', subject %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
